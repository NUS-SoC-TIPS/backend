{
  "name": "tips-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:dev:setup": "dotenv -e .env.development -- prisma migrate deploy && yarn pnpify prisma generate",
    "prisma:test:setup": "dotenv -e .env.test -- prisma migrate deploy && yarn pnpify prisma generate",
    "prisma:dev:migrate": "dotenv -e .env.development -- prisma migrate dev --skip-generate && yarn pnpify prisma generate",
    "prisma:dev:migrate:manual": "dotenv -e .env.development -- prisma migrate dev --create-only",
    "prisma:dev:debug": "dotenv -e .env.development -- prisma debug",
    "prisma:prod:migrate": "dotenv -e .env.production -- prisma migrate deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 1 && yarn prisma:dev:setup",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 1 && yarn prisma:test:setup",
    "commit": "git-cz",
    "build": "rimraf dist && yarn pnpify prisma generate && nest build",
    "start": "rimraf dist && dotenv -e .env.development -- nest start",
    "start:dev": "rimraf dist && dotenv -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "lint": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" && eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn db:test:restart && yarn test:src && yarn test:e2e",
    "test:ci": "yarn prisma:test:setup && yarn test:src",
    "test:src": "dotenv -e .env.test -- jest --runInBand --detectOpenHandles",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles",
    "postinstall": "husky && yarn run patch",
    "patch": "patch -p1 -N -i patches/*.patch || rimraf .yarn/**/*.rej"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/websockets": "^10.3.8",
    "@prisma/client": "5.14.0",
    "agora-access-token": "^2.0.4",
    "atob": "^2.1.2",
    "axios": "^1.7.2",
    "btoa": "^1.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.11",
    "firebase-admin": "^12.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lib0": "^0.2.94",
    "nest-winston": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "random-word-slugs": "^0.1.7",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@eslint/js": "^9.4.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/atob": "^2.1.4",
    "@types/btoa": "^1.2.5",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.13.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^7.11.0",
    "@yarnpkg/pnpify": "^4.1.0",
    "commitizen": "^4.3.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-x": "^0.5.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.5",
    "markdownlint-cli": "^0.41.0",
    "pactum": "^3.7.0",
    "prettier": "^3.3.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "5.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.prisma": [
      "prettier --write"
    ],
    "**/*.md": [
      "markdownlint --fix"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
